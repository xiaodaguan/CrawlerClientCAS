<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper.TestMapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="common.mapper.BaseMapper">
    <select id="getAllMD5ByType" parameterType="map"
            resultType="String" statementType="STATEMENT">
        select md5 from ${tablename} where rownum &lt; 10
    </select>
    <select id="getTotalCountByType" parameterType="map" resultType="int" statementType="STATEMENT">
        select count(id) from ${tablename}
    </select>

    <select id="selectAllSearchKeyword" resultType="common.task.SearchKey">
        select * from search_keyword where trunc(propose_time) >= trunc(sysdate) - 7 order by priority desc
    </select>



</mapper>
