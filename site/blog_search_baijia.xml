<?xml version="1.0" encoding="utf-8"?>
<site>
	<prop name="site" value="baidu"/>
	<prop name="siteName" value="baidu_blog_search"/>
	<prop name="siteFlag" value="5"/>
	<prop name="charset" value="utf-8"/>
	<prop name="downInterval" value="10"/>
	<prop name="page" value="10"/>
	<prop name="domainId" value="3"/>
	<prop name="agent" value="false"/>
	<prop name="threadNum" value="1"/>
	
	
	<prop name="accessUrl" value="http://baijia.baidu.com/?tn=search&amp;word=&lt;keyword&gt;"/>

	<!-- 列表页 -->
	<prop name="title" value="//H3/A"/>
	<prop name="url" value="//H3/A/@href"/>
	<prop name="brief" value="//P[@class='feeds-item-text1']"/>
	<prop name="pubtime" value="//SPAN[@class='tm']"/>
	<prop name="next" value=""/>
	<prop name="next_prefix" value="http://baijia.baidu.com/"/>
	
	
	<!-- 需要去掉{.} -->
	<!-- 内容页 -->
	<prop name="imgs_url" value="//P[@class='image']/IMG/@src"/>
	<prop name="author" value="//A[@class='name']"/>
	<prop name="content" value="//DIV[@class='article-detail']"/>
	<prop name="openId" value=""/> 
</site>
